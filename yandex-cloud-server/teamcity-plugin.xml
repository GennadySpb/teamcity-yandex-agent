<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>cloud-yandex</name>
    <display-name>Yandex Cloud Agents</display-name>
    <version>@Version@</version>
    <description>Allows using Yandex Compute Engine instances to scale the pool of TeamCity build agents</description>
    <download-url>https://github.com/vasiliy-briginets/teamcity-yandex-agent</download-url>
    <vendor>
      <name>Vasiliy Briginets</name>
      <url>https://github.com/vasiliy-briginets/teamcity-yandex-agent/</url>
    </vendor>
  </info>
  <requirements min-build="40000" />
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
